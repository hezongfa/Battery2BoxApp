<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/x2"
    android:background="@android:color/white"
    android:padding="@dimen/x20">

    <ImageView
        android:id="@+id/item_use_time_iv"
        android:layout_width="@dimen/x90"
        android:layout_height="@dimen/x90"
        android:layout_marginStart="@dimen/x15"
        android:layout_marginEnd="@dimen/x15"
        android:src="@mipmap/ic_use_time" />

    <TextView
        android:id="@+id/item_use_time_order_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@id/item_use_time_iv"
        android:text="@string/wallet_22"
        android:textColor="@android:color/black"
        android:textSize="@dimen/x28" />

    <TextView
        android:id="@+id/item_use_time_des_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/item_use_time_order_tv"
        android:layout_marginTop="@dimen/x10"
        android:layout_toEndOf="@id/item_use_time_iv"
        android:text="充电宝付费"
        android:textColor="@android:color/black"
        android:textSize="@dimen/x32" />

    <TextView
        android:id="@+id/item_use_time_c_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/item_use_time_des_tv"
        android:layout_marginTop="@dimen/x10"
        android:layout_toEndOf="@id/item_use_time_iv"
        android:text="长城公司"
        android:textColor="@color/gray_333333"
        android:textSize="@dimen/x26" />

    <TextView
        android:id="@+id/item_use_time_time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/item_use_time_c_tv"
        android:layout_marginTop="@dimen/x10"
        android:layout_toEndOf="@id/item_use_time_iv"
        android:text="2019-08-20 09:40"
        android:textColor="@color/gray_333333"
        android:textSize="@dimen/x26" />

    <TextView
        android:id="@+id/item_use_time_d_time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/x45"
        android:text="-1分钟"
        android:textColor="@android:color/black"
        android:textSize="@dimen/x32" />

    <TextView
        android:id="@+id/item_use_time_l_time_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/item_use_time_time_tv"
        android:layout_alignParentEnd="true"
        android:text="@string/wallet_23"
        android:textColor="@color/gray_333333"
        android:textSize="@dimen/x28" />
</RelativeLayout>